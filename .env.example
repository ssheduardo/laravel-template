MAIL_MAILER=smtp
MAIL_HOST=laravel_mail
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=www-admin@foo.example.com
MAIL_FROM_NAME="${APP_NAME}"

HTTP_PORT=80
HTTPS_PORT=443

JWT_SECRET=
